<?xml version="1.0"?>

<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          xsi:schemaLocation="http://www.enova.pl/schema/ http://www.enova.pl/schema/form.xsd"
          RightName="Page:NieruchomoscPage"
          Priority="44"
          DataType="ProjectMZGM.Nieruchomosc,ProjectMZGM">
  
  <Page Name="NieruchomoscPage" CaptionHtml="Najemca" DataContext="{DataSource}">
    <Group>
      <Row>
        <Stack>
          <Field EditValue="{UlicaNajemcy}" CaptionHtml="Ulica" OuterWidth="50" />
          <Field EditValue="{NumerBramyNajemcy}" CaptionHtml="Numer bramy" OuterWidth="50"/>
          <Field EditValue="{NumerMieszkaniaNajemcy}" CaptionHtml="Numer mieszkania" OuterWidth="50"/>
          <Field EditValue="{KodPocztowyNajemcy}" CaptionHtml="Kod pocztowy" OuterWidth="50"/>
          <Field EditValue="{MiastoNajemcy}" CaptionHtml="Miasto" OuterWidth="50"/>
        </Stack>
        <Stack>
          <Field CaptionHtml="Opis dodatkowy" EditValue="{OpisDodatkowy}" Class="LeftAlign" Width="*" Height="7"/>
        </Stack>
      </Row>

    </Group>

  </Page>
</DataForm>

